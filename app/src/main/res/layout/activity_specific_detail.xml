<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context=".SpecificDetailActivity">    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="15dp"            android:layout_marginTop="15dp"            android:layout_marginRight="15dp"            android:orientation="horizontal">            <ImageButton                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="2dp"                android:layout_marginTop="2dp"                android:layout_marginRight="2dp"                android:layout_marginBottom="3dp"                android:layout_weight="7"                android:background="@color/white"                android:src="@drawable/ic_baseline_arrow_back_ios_24"                tools:src="@drawable/ic_baseline_arrow_back_ios_24" />            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="1"                android:gravity="center"                android:text="2019 BMW X3 xDrive 30i Sport Utility 4D"                android:textSize="18dp" />        </LinearLayout>        <ImageView            android:layout_width="match_parent"            android:layout_height="220dp"            android:layout_marginLeft="20dp"            android:layout_marginTop="15dp"            android:layout_marginRight="20dp"            android:baselineAligned="false"            android:scaleType="fitCenter"            android:src="@drawable/bmw_x3_m_review_44"            app:srcCompat="@drawable/bmw_x3_m_review_44" />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:layout_marginTop="20dp"            android:layout_marginRight="15dp"            android:orientation="horizontal">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="1"                android:orientation="vertical">                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:text="2019 BMW X3 xDrive 30i Sport Utility 4D"                    android:textSize="22dp" />                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="3"                        android:text="Stock"                        android:textSize="13dp" />                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:text="62937293A"                        android:textSize="13dp" />                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="3"                        android:text="VIN"                        android:textSize="13dp" />                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:text="5UKDF9EJND7ENF"                        android:textSize="13dp" />                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="3"                        android:text="Color"                        android:textSize="13dp" />                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:text="Blue"                        android:textSize="13dp" />                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="3"                        android:text="Mileage"                        android:textSize="13dp" />                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:text="35,269"                        android:textSize="13dp" />                </LinearLayout>            </LinearLayout>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="2"                android:gravity="right"                android:orientation="vertical">                <CheckBox                    android:id="@+id/checkBoxPassedFailed"                    android:layout_width="100dp"                    android:layout_height="25dp"                    android:layout_marginLeft="15dp"                    android:layout_marginTop="5dp"                    android:layout_marginRight="15dp"                    android:background="@drawable/round"                    android:button="@drawable/ic_baseline_check_circle_24"                    android:buttonTint="#78F41D"                    android:text="Passed" />                <CheckBox                    android:id="@+id/checkBoxFailed"                    android:layout_width="100dp"                    android:layout_height="25dp"                    android:layout_marginLeft="15dp"                    android:layout_marginTop="5dp"                    android:layout_marginRight="15dp"                    android:background="@drawable/round_red"                    android:button="@drawable/ic_failed"                    android:buttonTint="#F62121"                    android:text="Failed" />                <TextView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="10dp"                    android:layout_marginRight="10dp"                    android:gravity="right"                    android:text="Mon, Oct 05, 2020 at 4:40PM PST"                    android:textSize="13dp" />            </LinearLayout>        </LinearLayout>        <com.google.android.material.card.MaterialCardView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:layout_marginTop="15dp"            android:layout_marginRight="20dp"            app:cardCornerRadius="10dp">            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="10dp"                android:text="Submit Repaire Order"                android:textSize="18dp"                app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24" />        </com.google.android.material.card.MaterialCardView>        <com.google.android.material.card.MaterialCardView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:layout_marginTop="15dp"            android:layout_marginRight="20dp"            app:cardCornerRadius="10dp">            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="10dp"                android:text="View Inspection"                android:textSize="18dp"                app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24" />        </com.google.android.material.card.MaterialCardView>        <com.google.android.material.card.MaterialCardView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:layout_marginTop="15dp"            android:layout_marginRight="20dp"            app:cardCornerRadius="10dp">            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="10dp"                android:text="Print Inspection"                android:textSize="18dp"                app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24" />        </com.google.android.material.card.MaterialCardView>        <com.google.android.material.card.MaterialCardView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:layout_marginTop="15dp"            android:layout_marginRight="20dp"            app:cardCornerRadius="10dp">            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="10dp"                android:text="View Factfile"                android:textSize="18dp"                app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24" />        </com.google.android.material.card.MaterialCardView>        <com.google.android.material.card.MaterialCardView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="20dp"            android:layout_marginTop="15dp"            android:layout_marginRight="20dp"            app:cardCornerRadius="10dp">            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_margin="10dp"                android:text="Print Factfile"                android:textSize="18dp"                app:drawableRightCompat="@drawable/ic_baseline_arrow_forward_ios_24" />        </com.google.android.material.card.MaterialCardView>    </LinearLayout>    </ScrollView></androidx.constraintlayout.widget.ConstraintLayout>