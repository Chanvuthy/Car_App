<?xml version="1.0" encoding="utf-8"?><RelativeLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_margin="5dp"    android:id="@+id/parent1">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <View            android:layout_width="match_parent"            android:layout_height="1dp"            android:layout_marginLeft="5dp"            android:layout_marginTop="5dp"            android:layout_marginRight="5dp"            android:background="@color/browm" />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginTop="5dp"            >            <TextView                android:id="@+id/txtNumDetail"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="5dp"                android:text="1" />            <TextView                android:layout_width="5dp"                android:layout_height="wrap_content"                android:text="." />            <TextView                android:id="@+id/txtDescDetail"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="2dp"                android:layout_marginRight="5dp"                android:layout_weight="1"                android:text="Confirm the engine warning light is not on under normal operation" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="5dp"            android:layout_marginTop="5dp"            android:layout_marginRight="5dp"            android:orientation="horizontal">            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="1"/>            <CheckBox                android:id="@+id/cB_NoIssue"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:scaleX="2"                android:scaleY="1.5"                android:layout_weight="2.5"                android:buttonTint="#5BE651" />            <CheckBox                android:id="@+id/cB_Issue"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="2.5"                android:scaleX="2"                android:scaleY="1.5"                android:buttonTint="#F13C3C" />            <CheckBox                android:id="@+id/cB_NA"                android:scaleX="2"                android:scaleY="1.5"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="2.5"                android:buttonTint="#B1ADAD" />        </LinearLayout>    </LinearLayout></RelativeLayout>