<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:layout_margin="5dp"    android:id="@+id/parent1">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="vertical"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <View            android:layout_width="match_parent"            android:layout_height="1dp"            android:layout_marginLeft="5dp"            android:layout_marginTop="5dp"            android:layout_marginRight="5dp"            android:background="@color/browm" />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content">            <TextView                android:id="@+id/txtNumDetail"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="5dp"                android:layout_weight="20"                android:text="1." />            <TextView                android:id="@+id/txtDescDetail"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_marginLeft="2dp"                android:layout_marginRight="5dp"                android:layout_weight="1"                android:text="Confirm the engine warning light is not on under normal operation" />        </LinearLayout>        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginLeft="5dp"            android:layout_marginRight="15dp"            android:orientation="horizontal">            <TextView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="1"/>            <CheckBox                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="3"                android:buttonTint="#5BE651" />            <CheckBox                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="3"                android:buttonTint="#F13C3C" />            <CheckBox                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_weight="3"                android:buttonTint="#B1ADAD" />        </LinearLayout>    </LinearLayout></androidx.constraintlayout.widget.ConstraintLayout>